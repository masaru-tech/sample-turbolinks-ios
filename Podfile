# platform :ios, '9.0'

target 'sample-turbolinks' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for sample-turbolinks
  pod 'Turbolinks', :git => 'https://github.com/turbolinks/turbolinks-ios.git', :branch => 'swift-3.0'

  target 'sample-turbolinksTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'sample-turbolinksUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
